{
    "extends": "eslint-config-fluid",
    "globals": {
        "fluid": true
    },
    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 10
    },
    "overrides": [{
        "files": ["src/assets/scripts/app.js", "src/assets/scripts/**/*.module.js"],
        "parserOptions": {
            "sourceType": "module"
        }
    }]
}
